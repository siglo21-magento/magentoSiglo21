<script type="text/javascript">

	function plusQty(qtyInput){
		require([
		'jquery'
		], function($){
			if($('#qty_'+qtyInput).attr('disabled')!='disabled'){
				//total = 0;
				qty = $('#qty_'+qtyInput).attr('value');
				price = $('#product-price-'+qtyInput).attr('data-price-amount');
				qty = parseInt(qty);
				price = parseFloat(price);
				qty++;
				price = price * qty;
				$('#qty_'+qtyInput).val(qty);
				$('#total_price_'+qtyInput).text('$'+price.toFixed(2));
				$('#addto_'+qtyInput).val(qty);
				$('#sticky_qty').val(qty);
			}
		});

	}
	function minusQty(qtyInput){
		require([
		'jquery'
		], function($){
			if($('#qty_'+qtyInput).attr('disabled')!='disabled'){
				qty = $('#qty_'+qtyInput).attr('value');
				qty = parseInt(qty);
				price = $('#product-price-'+qtyInput).attr('data-price-amount');
				price = parseFloat(price);
				qty--;
				price = price * qty;
				if(qty >= 0){
					$('#qty_'+qtyInput).val(qty);
					$('#total_price_'+qtyInput).text('$'+price.toFixed(2));
					$('#addto_'+qtyInput).val(qty);
					$('#sticky_qty').val(qty);
				}
			}
		});
	}
	function calc(qtyInput){
		require([
		'jquery'
		], function($){
			if($('#qty_'+qtyInput).attr('disabled')!='disabled'){
				qty = $('#qty_'+qtyInput).attr('value');
				qty = parseInt(qty);
				price = $('#product-price-'+qtyInput).attr('data-price-amount');
				price = parseFloat(price)
				price = price * qty;
				$('#total_price_'+qtyInput).text('$'+price.toFixed(2));
				$('#addto_'+qtyInput).val(qty);
				$('#sticky_qty').val(qty);
			}
		});
	}
</script>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer.wishlist.item.comment" remove="true"/>
        <referenceBlock name="customer.wishlist.item.edit" remove="true"/>
        <referenceBlock name="customer.wishlist.item.name" remove="true"/>
        <referenceBlock name="customer.wishlist.items">
            <block class="Magento\Wishlist\Block\Customer\Wishlist\Item\Column\Info" name="customer.wishlist.item.stock" template="item/column/stock.phtml" after="customer.wishlist.item.price" cacheable="false"/>
        </referenceBlock>
    </body>
</page>

        <referenceContainer name="content">
            <block class="Aventi\SAP\Block\Invoice\History" name="sap.invoice.history" cacheable="false" template="Aventi_SAP::invoice/history.phtml"/>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="Magento_Customer::account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>

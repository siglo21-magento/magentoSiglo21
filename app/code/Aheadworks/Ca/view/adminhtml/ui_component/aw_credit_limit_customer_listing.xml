        <column name="aw_ca_company" sortOrder="21">
            <settings>
                <filter>text</filter>
                <label translate="true">Company Name</label>
                <dataType>text</dataType>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="aw_ca_is_admin" sortOrder="22" component="Magento_Ui/js/grid/columns/select">

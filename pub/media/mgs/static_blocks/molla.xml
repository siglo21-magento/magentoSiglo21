	<item>
		<title><![CDATA[Block size guide]]></title>
		<identifier><![CDATA[product_block_size_guide]]></identifier>
		<content><![CDATA[<div class="extra-link">
<a href="#" class="size-guide"><i class="icon-th-list"></i>size guide</a>
</div>
<div id="extra-product-block" class="extra-block" style="display: none;">
<img class="img-responsive" src="{{media url="wysiwyg/sizecharts.png"}}" alt="" />
</div>
<script type="text/javascript">
    require(
        [
            'jquery',
            'Magento_Ui/js/modal/modal'
        ],
        function(
            $,
            modal
        ) {
			
			$('.extra-link').click(function (){
                $('body').css({
                    "overflow-x": "hidden",
                });
                $("#extra-product-block").removeClass('js-form-hidden');
                $("#extra-product-block").addClass('js-form-active');
				var options = {
					type: 'popup',
					responsive: true,
                    innerScroll: true,
                    modalClass: 'extra-product-block',
                    closed: function() {
                        $("#extra-product-block").removeClass('js-form-active');
                        $("#extra-product-block").addClass('js-form-hidden');
                    }
				};

				var popup = modal(options, $('#extra-product-block'));

				$('#extra-product-block').modal('openModal');
				});
        }
    );
</script>]]></content>
